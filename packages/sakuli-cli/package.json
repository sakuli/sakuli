{
  "name": "@sakuli/cli",
  "version": "2.1.2",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "typedoc": "typedoc --options ../../typedoc.js --out ../../docs/sakuli-cli src/",
    "test:watch": "jest --watch --coverage",
