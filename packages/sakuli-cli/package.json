  "license": "Apache-2.0",
  "dependencies": {
    "@sakuli/commons": "^2.1.2",
    "@sakuli/core": "^2.1.2",
    "@sakuli/legacy": "^2.1.2",
    "@sakuli/plugin-validator": "1.0.2",
    "ansi-escapes": "4.2.1",
    "chalk": "2.4.2",
    "date-fns": "1.30.1",
    "figlet": "1.2.3",
    "yargonaut": "1.1.4",
    "yargs": "12.0.5",
    "youch": "2.0.10",
    "youch-terminal": "1.0.0"
  },
  "devDependencies": {
