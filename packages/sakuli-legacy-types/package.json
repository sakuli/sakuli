{
	"name": "@sakuli/legacy-types",
	"version": "2.0.1",
	"description": "provide typings for Sakuli Legacy api (@sakuli/legacy)",
	"keywords": [
		"typings",
		"sakuli",
		"e2e",
		"ui",
		"testing",
		"legacy"
	],
	"typings": "./out/index",
	"scripts": {
		"build": "ts-node -P tsconfig.json src/build-dts-file",
		"typedoc": "typedoc --mode \"modules\" --includeDeclarations --excludeNotExported --entryPoint \"__global\" --out dist/docs out/"
	},
	"contributors": [
		"Tim Keiner <tim.keiner@consol.de>",
