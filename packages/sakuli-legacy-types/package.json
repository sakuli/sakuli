	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sakuli/sakuli.git"
	},
	"bugs": {
		"url": "https://github.com/sakuli/sakuli/issues"
	},
	"devDependencies": {
		"@sakuli/legacy": "^2.0.1",
		"@types/node": "^12.0.5",
		"dts-dom": "^3.3.0",
		"flatted": "^2.0.0",
		"ts-node": "8.2.0",
		"typescript": "^3.4.3",
		"typedoc": "^0.14.2"
